%section
	.container
		%h2.text-center Projects You Support
		.section-content
			.owl-carousel#owl-projects-you-support
				.project-card
					.header
						%a{href:"#"}
					.content
						.title
							%h4 Demo Project
						.body
							%p This is the demo card of a demo project lel
					.footer
						.labels
							%p Arts Comics ETC
						.status
							.progress-info
								%p 80%
							.progress{style:"height:10px;"}
								.progress-bar.progress-bar-success{"aria-valuenow": "80", "aria-valuemin": "0", "aria-valuemax": "100", style:"width: 80%;"}
						.status-info.col-md-12
							.col-md-6
								%p Status 1
							.col-md-6
								%p Status 2
		.section-footer
			%p.text-center
				%a.btn.btn-primary.btn-lg.btn-outline{href:"#"} Explore All